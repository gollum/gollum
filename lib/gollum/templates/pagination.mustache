<div id="pagination" class="pb-2 pl-3 pr-3">
	{{#previous_page}}
	  <span id="prev">
	    <a href="?page_num={{previous_page}}">&laquo; Previous</a>
	  </span>
	{{/previous_page}}

	<span>&nbsp;</span>

	{{#next_page}}
	  <span id="next" class="float-right">
	    <a href="?page_num={{next_page}}">Next &raquo;</a>
	  </span>
	{{/next_page}}
</div>
