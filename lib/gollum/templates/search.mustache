<div id="wiki-wrapper" class="results">
  <div id="head">
    {{>navbar}}
    <h1 class="py-4"><span class="f1-light text-gray-light">Search results for</span> {{name}}</h1>
  </div>

<div id="results">

{{#has_results}}
  <table>
  <thead><tr>
  <th>Filename</th>
  <th>Matches</th>
  </tr></thead>
  <tbody>
    {{#results}}
      <tr>
      <td><a href="{{base_url}}/{{name}}">{{name}}</a></td>
      <td>{{#context}}{{.}}<br/>{{/context}}</td>
      </tr>
    {{/results}}
  </tbody>
  </table>
{{/has_results}}

{{#no_results}}
  <p id="no-results">
    There are no results for your search <strong>{{query}}</strong>.
  </p>
{{/no_results}}

</div>
<div id="footer">
  <ul class="actions">
    <li class="minibutton"><a href="#">Back to Top</a></li>
  </ul>
</div>
</div>

